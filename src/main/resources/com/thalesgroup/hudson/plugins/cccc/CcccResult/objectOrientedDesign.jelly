<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define"
  xmlns:l="/lib/layout" xmlns:t="/lib/hudson" xmlns:f="/lib/form"
  xmlns:i="jelly:fmt" xmlns:local="local">
  <st:header name="Content-Type" value="text/html;charset=UTF-8" />
  <table class="pane sortable" id="files">
    <tr>
      <td class="pane-header">Module Name</td>
      <td class="pane-header" style="width:2em">WMC1</td>
      <td class="pane-header" style="width:2em">WMCv</td>
      <td class="pane-header" style="width:60%">DIT</td>
      <td class="pane-header" style="width:60%">NOC</td>
      <td class="pane-header" style="width:60%">CBO</td>
    </tr>
    <tbody>    
        <j:forEach var="module" items="${it.report.objectOrientedDesignModuleList}">    
	        <tr>
	          <td class="pane">${module.name}</td>
	          <td class="pane">${module.weightedMethodsPerClassUnity}</td>
	          <td class="pane">${module.weightedMethodsPerClassVisibility}</td>
	          <td class="pane">${module.depthOfInheritanceTree}</td>
	          <td class="pane">${module.numberOfChildren}</td>
	          <td class="pane">${module.couplingBetweenObjects}</td>
	        </tr>
		</j:forEach>
    </tbody>
  </table>


</j:jelly>